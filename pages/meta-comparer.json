{
  "title": "元数据比较器",
  "type": "page",
  "body": {
    "type": "crud",
    "draggable": true,
    "syncLocation": false,
    "autoFillHeight": true,
    "api": {
      "method": "get",
      "url": "/api/quality/meta_comparer"
    },
    "keepItemSelectionOnPageChange": true,
    "filter": {
      "title": "元数据对比参数",
      "body": [
        {
          "label": "对比类型：",
          "type": "select",
          "name": "type",
          "value": "TWO_GROUP_FORM",
          "options": [
            {
              "label": "TWO_GROUP_FORM",
              "value": "TWO_GROUP_FORM"
            },
            {
              "label": "TWO_GROUP_ENTITY",
              "value": "TWO_GROUP_ENTITY"
            }
          ]
        },
        {
          "label": "源分组：",
          "type": "select",
          "name": "group_a",
          "value": "tf-master-standard",
          "options": [
            {
              "label": "jdy-v7-dev",
              "value": "jdy-v7-dev"
            },
            {
              "label": "jdy-pro",
              "value": "jdy-pro"
            },
            {
              "label": "tf-feature1",
              "value": "tf-feature1"
            },
            {
              "label": "tf-master-standard",
              "value": "tf-master-standard"
            }
          ]
        },
        {
          "label": "目标分组：",
          "type": "select",
          "name": "group_b",
          "value": "jdy-pro",
          "options": [
            {
              "label": "jdy-v7-dev",
              "value": "jdy-v7-dev"
            },
            {
              "label": "jdy-pro",
              "value": "jdy-pro"
            },
            {
              "label": "tf-feature1",
              "value": "tf-feature1"
            },
            {
              "label": "tf-master-standard",
              "value": "tf-master-standard"
            }
          ]
        },
        {
          "type": "input-text",
          "label": "fnumber：",
          "name": "number_like",
          "value": "pur_bill_order",
          "placeholder": ">= 3个字符"
        }
      ]
    },
    "columns": [
      {
        "name": "name",
        "label": "比较对象",
        "width": 300
      },
      {
        "name": "key",
        "label": "fkey",
        "width": 600
      },
      {
        "name": "type",
        "label": "ftype",
        "width": 600
      },
      {
        "type": "operation",
        "label": "对比",
        "width": 40,
        "buttons": [
          {
            "type": "button",
            "label": "对比",
            "level": "primary",
            "actionType": "dialog",
            "dialog": {
              "type": "form",
              "size": "full",
              "title": "元数据比较",
              "body": {
                "type": "diff-editor",
                "name": "valueA",
                "diffValue": "${valueB}"
              }
            }
          }
        ]
      }
    ]
  }
}
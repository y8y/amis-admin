{
  "title": "ZK 配置中心比较器",
  "type": "page",
  "body": {
    "type": "crud",
    "draggable": true,
    "syncLocation": false,
    "autoFillHeight": true,
    "api": {
      "method": "get",
      "url": "/api/quality/zkconfig_comparer"
    },
    "keepItemSelectionOnPageChange": true,
    "filter": {
      "title": "分组选取",
      "body": [
        {
          "label": "源分组：",
          "type": "select",
          "name": "group_a",
          "value": "tf-master-standard",
          "options": [
            {
              "label": "jdy-v7-dev",
              "value": "jdy-v7-dev"
            },
            {
              "label": "jdy-pro",
              "value": "jdy-pro"
            },
            {
              "label": "tf-feature1",
              "value": "tf-feature1"
            },
            {
              "label": "tf-master-standard",
              "value": "tf-master-standard"
            }
          ]
        },
        {
          "label": "目标分组：",
          "type": "select",
          "name": "group_b",
          "value": "jdy-pro",
          "options": [
            {
              "label": "jdy-v7-dev",
              "value": "jdy-v7-dev"
            },
            {
              "label": "jdy-pro",
              "value": "jdy-pro"
            },
            {
              "label": "tf-feature1",
              "value": "tf-feature1"
            },
            {
              "label": "tf-master-standard",
              "value": "tf-master-standard"
            }
          ]
        }
      ]
    },
    "columns": [
      {
        "name": "path",
        "label": "比较对象",
        "width": 300,
        "className": "overflow-auto"
      },
      {
        "name": "valueA",
        "label": "源分组",
        "width": 600,
        "className": "overflow-auto"
      },
      {
        "name": "valueB",
        "label": "目标分组",
        "width": 600,
        "className": "overflow-auto"
      },
      {
        "type": "operation",
        "label": "对比",
        "width": 40,
        "buttons": [
          {
            "type": "button",
            "label": "对比",
            "level": "primary",
            "actionType": "dialog",
            "dialog": {
              "type": "form",
              "size": "full",
              "title": "配置项比较",
              "body": {
                "type": "diff-editor",
                "name": "valueA",
                "diffValue": "${valueB}"
              }
            }
          }
        ]
      }
    ]
  }
}